pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
            stage('build') {
            steps {
                echo 'building '
            }
            }
            stage('Deploy') {
            steps {
                echo 'Deploying World'
            }
            }
            stage('Test') {
            steps {
                echo 'Testing'
            }
            }
            stage('Release') {
            steps {
                echo 'Releasestage'
            }
            }
    }
        }
    

